{
    "name": "Knock Knock",
    "description" : "We’ll wake your desire up",
    "version": "1.0",
    "manifest_version": 2,
"description": "진짜 믿을 수 있는, 당신이 원하는 최저가!",
"icons":{ "128" :"icon.png" }, 
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
"background": {
       "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "identity"
  ],
  "oauth2": {
    "client_id": "172501072688-6q7ago0lhf99ldlgk0msal836mgnd2tg.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },

"browser_action": {
      "default_icon": "icon.png",
      "default_popup": "hello.html"
    },
    "content_scripts":[{
	"js": ["js/jquery.min.js", "js/textFit.min.js"],
	"matches": ["<all_urls>"]
    }],
    "permissions": [
        "activeTab", "<all_urls>", "tabs"
    ]
}


